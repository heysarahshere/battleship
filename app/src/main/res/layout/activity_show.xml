        android:layout_height="96dp"
        app:layout_constraintBottom_toBottomOf="@+id/sqThreeImage2"
        app:layout_constraintEnd_toEndOf="@+id/sqThreeImage2"
        app:layout_constraintHorizontal_bias="0.006"
        app:layout_constraintStart_toStartOf="@+id/sqThreeImage2"
        app:layout_constraintTop_toTopOf="@+id/sqThreeImage2"
        app:layout_constraintVertical_bias="1.0"
