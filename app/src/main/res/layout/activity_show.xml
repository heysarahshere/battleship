    <TextView
        android:id="@+id/expTextView"
        android:layout_width="119dp"
        android:layout_height="45dp"
        android:text="1243"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/sqFourImage"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="@+id/sqFourImage"
        app:layout_constraintTop_toBottomOf="@+id/expLabelTextView" />

    <TextView
        android:id="@+id/coinsTextView"
        android:layout_width="138dp"
        android:layout_height="44dp"
        android:layout_marginStart="4dp"
