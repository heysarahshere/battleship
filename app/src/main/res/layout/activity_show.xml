        app:layout_constraintTop_toBottomOf="@+id/sqThreeImage2"
        app:srcCompat="@color/cardview_shadow_start_color" />

    <ImageView
        android:id="@+id/sqThreeImage"
        android:layout_width="167dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sqOneImage"
        app:srcCompat="@color/cardview_shadow_start_color" />

    <ImageView
        android:id="@+id/sqThreeImage2"
        android:layout_width="387dp"
        android:layout_height="122dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
