        app:layout_constraintEnd_toEndOf="@+id/sqTwoImage"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/sqTwoImage"
        app:layout_constraintTop_toTopOf="@+id/sqTwoImage"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/wonTextView"
        android:layout_width="110dp"
        android:layout_height="44dp"
        android:text="1243"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/sqOneImage"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toStartOf="@+id/sqOneImage"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/tiedTextView"
        android:layout_width="110dp"
        android:layout_height="44dp"
        android:text="1243"
        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/sqThreeImage"
        app:layout_constraintHorizontal_bias="0.789"
