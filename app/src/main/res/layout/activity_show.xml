        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/sqThreeImage2"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/sqThreeImage2"
        app:layout_constraintVertical_bias="0.26" />

    <TextView
        android:id="@+id/expLabelTextView"
        android:layout_width="95dp"
        android:layout_height="27dp"
        android:text="Experience"
        android:textAlignment="center"
        android:textColor="@color/cardview_light_background"
        app:layout_constraintBottom_toBottomOf="@+id/sqFourImage"
        app:layout_constraintEnd_toEndOf="@+id/sqFourImage"
        app:layout_constraintHorizontal_bias="0.958"
        app:layout_constraintStart_toStartOf="@+id/sqFourImage"
        app:layout_constraintTop_toTopOf="@+id/sqFourImage"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="114dp"
        android:layout_height="30dp"
        android:text="Battles Lost"
        android:textAlignment="center"
        android:textColor="@color/cardview_light_background"
