    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="159dp"
        android:layout_height="129dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/sqThreeImage2"
        app:layout_constraintEnd_toEndOf="@+id/sqThreeImage2"
        app:layout_constraintTop_toTopOf="@+id/sqThreeImage2"
        app:layout_constraintVertical_bias="0.857"
        app:srcCompat="@drawable/level" />

    <TextView
        android:id="@+id/levelTextView"
        android:layout_width="106dp"
        android:layout_height="78dp"
        android:text="1"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintEnd_toEndOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.196" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="401dp"
        android:layout_height="90dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sqThreeImage"
        app:srcCompat="@drawable/footer" />

</androidx.constraintlayout.widget.ConstraintLayout>