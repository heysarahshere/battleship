        android:textAlignment="textEnd"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/sqTwoImage"
        app:layout_constraintEnd_toEndOf="@+id/sqTwoImage"
        app:layout_constraintHorizontal_bias="0.701"
        app:layout_constraintStart_toStartOf="@+id/sqTwoImage"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="88dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="@+id/sqOneImage"
        app:layout_constraintEnd_toEndOf="@+id/sqOneImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sqOneImage"
        app:layout_constraintTop_toTopOf="@+id/sqOneImage"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/win" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="62dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="@+id/sqThreeImage"
        app:layout_constraintEnd_toEndOf="@+id/sqThreeImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sqThreeImage"
        app:layout_constraintTop_toTopOf="@+id/sqThreeImage"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/tie" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="68dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="@+id/sqFourImage"
        app:layout_constraintEnd_toEndOf="@+id/sqFourImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sqFourImage"
        app:layout_constraintTop_toTopOf="@+id/sqFourImage"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/coin2" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="69dp"
        android:layout_height="87dp"
        app:layout_constraintBottom_toBottomOf="@+id/lostTextView"
        app:layout_constraintEnd_toEndOf="@+id/sqTwoImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sqTwoImage"
        app:layout_constraintTop_toTopOf="@+id/sqTwoImage"
        app:layout_constraintVertical_bias="0.733"
